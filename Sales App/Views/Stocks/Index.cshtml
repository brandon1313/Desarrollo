@model IEnumerable<Sales_App.Models.Stock>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Items.NameItem)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NewEntry.CorrelativeBill)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrderMaster.OrderMasterID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.In)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Out)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Items.NameItem)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NewEntry.CorrelativeBill)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrderMaster.OrderMasterID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.In)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Out)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
